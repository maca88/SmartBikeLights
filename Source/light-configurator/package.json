    "@material-ui/icons": "^5.0.0-alpha.15",
    "@material-ui/lab": "^5.0.0-alpha.16",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "big-integer": "^1.6.50",
    "date-fns": "^2.16.1",
    "env-cmd": "^10.1.0",
    "leaflet": "^1.7.1",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.1.6",
    "nanoid": "^3.1.15",
