  "scripts": {
    "start-datafield": "env-cmd -e datafield npm start",
    "build-datafield": "env-cmd -e datafield npm run build",
    "publish-datafield": "gh-pages -f -d build -r https://github.com/maca88/SmartBikeLights.git",
    "deploy-datafield": "npm run build-datafield && npm run publish-datafield",
    "start-widget": "env-cmd -e widget npm start",
    "build-widget": "env-cmd -e widget npm run build",
    "publish-widget": "gh-pages -f -d build -r https://github.com/maca88/BikeLightsControl.git",
    "deploy-widget": "npm run build-widget && npm run publish-widget",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.1.0"
  }
